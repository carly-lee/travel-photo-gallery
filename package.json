{
  "name": "vertical-horizontal-mix-scroll",
  "version": "1.0.0",
  "description": "Vertical horizontal mix scroll",
  "main": "index.js",
  "scripts": {
    "start": "node ./server.js",
    "build": "webpack --config ./webpack.config.prod.js",
    "test": "NODE_ENV=test BABEL_DISABLE_CACHE=1 jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carly-lee/vh-mix-scroll.git"
  },
  "keywords": [
    "react",
    "scroll",
    "gallery"
  ],
  "author": "Carly Lee",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/carly-lee/vh-mix-scroll/issues"
  },
  "homepage": "https://github.com/carly-lee/vh-mix-scroll#readme",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-preset-react-app": "^1.0.0",
    "clean-webpack-plugin": "^0.1.14",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.2",
    "eslint-config-react-app": "^0.3.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.28.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^17.0.3",
    "json-loader": "^0.5.4",
    "nock": "^9.0.2",
    "postcss-loader": "^1.1.1",
    "postcss-smart-import": "^0.6.0",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^1.3.0",
    "redux-mock-store": "^1.2.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "react-app",
      "es2015"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "babel-plugin-webpack-loaders",
            {
              "config": "./webpack.config.test.js",
              "verbose": false
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "automock": false,
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "verbose": true,
    "modulePaths": [
      "<rootDir>/src/",
      "<rootDir>/src/app/",
      "<rootDir>/src/app/actions/",
      "<rootDir>/src/app/components/",
      "<rootDir>/src/app/reducers/",
      "<rootDir>/src/app/utils/"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.css$": "identity-obj-proxy"
    },
    "testPathDirs": [
      "<rootDir>/src/test/"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
